
<head>
<link rel="stylesheet" href="/css/styles.css">
</head>
<body>
<div class="container">
    <h3 class="display-4">Encuentra tu curp registrada</h1>
<!-- Search form -->
<form action="perazaAlicia361/buscar" method="GET" class="form-inline d-flex md-form form-sm" >
 
    <input class="form-control form-control-sm ml-2 w-50" type="text" placeholder="Busca tu curp"
      aria-label="Search" name="busca" style="float: right;"> 
      <button type="submit" class="btn btn-primary boton ml-2 w-30" style="border-radius: 5px;
      background-color: rgb(230, 197, 146); margin-top:10px; margin-bottom: 20px; border-color: rgb(255, 255, 255);" >Buscar</button>
  </form>
  
  
  
  
  <table id="myTable" class="table table-striped table-responsive-md btn-table">
  <thead>
  <tr>
  <th class="th-sm" >Nombre</th>
  <th class="th-sm">Apellido Paterno</th>
  <th class="th-sm">Apellido Materno</th>
  <th class="th-sm">Fecha de Nacimiento</th>
  <th class="th-sm">Genero</th>
  <th class="th-sm">Estado</th>
  <th class="th-sm">Curp</th>
  </tr>
  </thead>
  <tbody>

    <% all.forEach(function(all){  %>
        <tr>
          <td><%= all.nombre %></td>
          <td><%= all.paterno %></td>
          <td><%= all.materno %></td>
          <td><%= all.birthday %></td>
          <td><%= all.gender %></td>
          <td><%= all.estado %></td>
          <td><%= all.curp %></td>
          
        </tr>
<%})%>

   </tbody>
  </table>
 </div>
</body>